                        </table>
                </div>
            </div>
        </div>
    </section>
    <!--End Main -->

<!--Đăng ký Thành Viên-->

    <div class="modal fade" id="DkyThanhVien" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">Đăng ký thành viên</h5>
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
        </div>
        
        <div class="modal-body">
            <form class="formDkyTV" action="http://localhost:5000/signMember" method="GET">
                <div class="form-group row">
                    <label for="hVTen" class="col-3 labelName">Họ và tên</label>
                    <input type="text" class="form-control col-8" name="hVTen" id="hVTen" placeholder="Nguyễn A" required>
                </div>

                <div class="form-group row">
                    <label for="gTinh" class="col-3 labelName">Giới tính</label>
                    <select id="gTinh" name="gTinh" class="form-control col-8" required>
                        <option>Nam</option>
                        <option>Nữ</option>
                    </select>
                </div>

                <div class="form-group row">
                    <label for="soCMND" class="col-3 labelName">Số CMND</label>
                    <input type="text" class="form-control col-8" id="soCMND" name="soCMND"placeholder="123456789" required>
                </div>

                <div class="form-group row">
                    <label for="sdThoai" class="col-3 labelName">Số điện thoại</label>
                    <input type="text" class="form-control col-8" id="sdThoai" name="sdThoai" placeholder="0123456789" required>
                </div>

                <div class="form-group row">
                    <label for="dToc" class="col-3 labelName">Dân tộc</label>
                    <input type="text" class="form-control col-8" id="dToc" name="dToc" placeholder="Kinh" required>
                </div>

                <div class="form-group row">
                    <label for="ngaySinh" class="col-3 labelName">Ngày sinh</label>
                    <input class="form-control col-8" type="date" id="ngaySinh" name="ngaySinh" required>
                </div>

                <div class="form-group row">
                    <label for="eMail" class="col-3 labelName">Email</label>
                    <input type="email" class="form-control col-8" id="eMail" name="eMail" placeholder="abc@gmail.com" required>
                </div>

                <div class="form-group row">
                    <label for="diaChi" class="col-3 labelName">Địa chỉ</label>
                    <textarea class="form-control col-8" id="diaChi" name="diaChi" rows="5" placeholder="Địa chỉ" required></textarea>
                </div>

                <div class="form-group">
                    <label for="anh" class="col-3 labelName">Chọn ảnh</label>
                    <div class="col-8">
                        <img id="anh" alt="your image" width="100px" height="100px" />
                        <input type="file"  name='ima' onchange="document.getElementById('anh').src = window.URL.createObjectURL(this.files[0])">
                    </div>
                </div>

                <button type="submit" class="btn btn-primary float-right">Đăng ký</button>
                
            </form>


        </div>

        </div>
    </div>
    </div>
<!--End Đăng ký Thành Viên-->


<!--Chỉnh sửa sách-->
    {{#each bookAll}}
    <div class="modal fade" id="chinhSuaSach{{stt}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">Chỉnh sửa Sách</h5>
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
        </div>

        <div class="modal-body">
            <form class="formDkyTV" action="http://localhost:5000/editBook" method="GET">
                <input type="hidden" id="{{id}}" name="soIDBook" value="{{id}}">

                <div class="form-group row">
                    <label for="etenSach" class="col-3 labelName">Tên sách</label>
                    <input type="text" class="form-control col-8" name="etenSach" id="etenSach" value="{{tensach}}">
                </div>

                <div class="form-group row">
                    <label for="etenTacGia" class="col-3 labelName">Tên tác giả</label>
                    <input type="text" class="form-control col-8" name="etenTacGia" id="etenTacGia" value="{{tentacgia}}">
                </div>

                <div class="form-group row">
                    <label for="etLoai" class="col-3 labelName">Thể loại</label>
                    <input type="text" class="form-control col-8" id="etLoai" name="etLoai" value="{{theloai}}">
                </div>

                <div class="form-group row">
                    <label for="esoLuong" class="col-3 labelName">Số lượng</label>
                    <input type="text" class="form-control col-8" id="esoLuong" name="esoLuong" value="{{soluong}}">
                </div>

                <div class="form-group row">
                    <label for="engayNhap" class="col-3 labelName">Ngày nhập</label>
                    <input type="text" class="form-control col-8" id="engayNhap" name="engayNhap" value="{{ngaynhap}}" >
                </div>

                <div class="form-group row">
                    <label for="emoTaSach" class="col-3 labelName">Mô tả</label>
                    <textarea class="form-control col-8" id="emoTaSach" name="emoTaSach" rows="5">{{mota}}</textarea>
                </div>

                <div class="form-group">
                    <label for="anh" class="col-3 labelName">Chọn ảnh</label>
                    <div class="col-8">
                        <img id="anhbook{{id}}" alt="your image" width="100px" height="100px" src="{{image}}"/>
                        <input type="file"  name='eimabook' onchange="document.getElementById('anhbook{{id}}').src = window.URL.createObjectURL(this.files[0])">
                    </div>
                </div>

                <button type="submit" class="btn btn-primary float-right">Đồng ý</button>
                
            </form>


        </div>

        </div>
    </div>
    </div>
    {{/each}}
<!--End Chỉnh sửa sách-->

<!--Chi tiết sách-->
    {{#each bookAll}}
    <div class="modal fade" id="chiTietSach{{stt}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">Thông tin Sách</h5>
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
        </div>
        <div class="modal-body">
            <form class="formDkyTV">

                <div class="form-group row">
                    <img id="anh" class="img-thumbnail" src="{{image}}" alt="" style="width: 200px; height: 200px; overflow: hidden; margin: 0 auto;">  
                </div>  

                <div class="form-group row">
                    <label for="tenSach" class="col-3 labelName">Tên sách</label>
                    <input type="text" class="form-control col-8" name="tenSach" id="tenSach" placeholder="{{tensach}}" readonly>
                </div>

                <div class="form-group row">
                    <label for="tenTacGia" class="col-3 labelName">Tên tác giả</label>
                    <input type="text" class="form-control col-8" name="tenTacGia" id="tenTacGia" placeholder="{{tentacgia}}" readonly>
                </div>

                <div class="form-group row">
                    <label for="tLoai" class="col-3 labelName">Thể loại</label>
                    <input type="text" class="form-control col-8" id="tLoai" name="tLoai" placeholder="{{theloai}}" readonly>
                </div>

                <div class="form-group row">
                    <label for="soLuong" class="col-3 labelName">Số lượng</label>
                    <input type="text" class="form-control col-8" id="soLuong" name="soLuong" placeholder="{{soluong}}" readonly>
                </div>

                <div class="form-group row">
                    <label for="ngayNhap" class="col-3 labelName">Ngày nhập</label>
                    <input type="email" class="form-control col-8" id="ngayNhap" name="ngayNhap" placeholder="{{ngaynhap}}" readonly>
                </div>

                <div class="form-group row">
                    <label for="moTaSach" class="col-3 labelName">Mô tả</label>
                    <textarea class="form-control col-8" id="moTaSach" name="moTaSach" rows="5" placeholder="{{mota}}" readonly></textarea>
                </div>
            </form>
        </div>
        </div>
    </div>
    </div>
    {{/each}}
<!--End Chi tiết sách-->