    <!--Start Main -->
    <section id="mainAdmin">
        <div class="container">
            <div class="row">
                <div class="col-3 col-sm-3 col-md-3 col-lg-3 col-xl-3">
                    <div class="list-group">
                        <p href="#" class="list-group-item list-group-item-action listBang" style="background-color:#f94f2e; color: white;"><i class="fas fa-home"></i>  Bảng điều khiển</p>
                        <button type="button" class="list-group-item list-group-item-action listBang" data-toggle="modal" data-target="#DkyThanhVien"><i class="fas fa-user-plus"></i>  Đăng ký thành viên</button>
                        <a href="/quanlynhanvien" class="list-group-item list-group-item-action listBang "><i class="fas fa-user"></i>   Quản lý nhân viên</a>
                        <a href="/quanlydocgia" class="list-group-item list-group-item-action listBang "><i class="fas fa-user"></i>  Quản lý độc giả</a>
                        <a href="/quanlysach" class="list-group-item list-group-item-action listBang "><i class="fas fa-book"></i>  Quản lý sách</a>
                        <a href="/thongke" class="list-group-item list-group-item-action listBang "><i class="fas fa-coins"></i> Thống kê </a>
                    </div>
                </div>

                <div class="col-9 col-sm-9 col-md-9 col-lg-9 col-xl-9">
                    <div class="mucTongQuan">
                        <h3>Danh sách sách</h3>
                    </div>
                    <div class="row quanLyHienThiThongTin">
                        <form class="form-inline mx-auto">
                            <input class="form-control formSearch" type="text" placeholder="Nhập nội dung cần tìm" style="padding-right: 60px;">
                            <button class="btn btn-outline-success " type="submit" style="margin-left: 37px;">Tìm kiếm</button>
                        </form>
                    </div>
                    
                    <table class="table bangNhanVien">
                        <thead>
                            <tr>
                                <th>Tên sách</th>
                                <th>Tác giả</th>
                                <th>Thể loại</th>
                                <th>Tùy chỉnh</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each bookAll}}
                            <tr>
                                <td>{{tensach}}</td>
                                <td>{{tentacgia}}</td>
                                <td>{{theloai}}</td>

                                <td>
                                    <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#chinhSuaSach{{stt}}">Edit</button>
